INSERT INTO `ComedyDB`.`Archive` (`rid`, `text`, `type`, `uid`, `mainAuthor`, `timestamp`) VALUES ('0', 'На черный КВН&#44; миниатюра &#34;Самоубийца&#34;.<br><i>/На стуле сидит парень с пистолетом. Затем он прикладывает его к виску&#44; потом засовывает в рот с явной целью застрелиться. Потом он откладывает это занятье&#44; достает презерватив и надевает его на дуло пистолета. После чего он засовывает пистолет в рот&#44; раздается выстрел&#44; бошка парня дергается и он замерает в позе мертвого. Пару секунд спустя&#44; он очухивается&#44; снимает презерватив с спистолета&#44; и характерным жестом завязывает его как использованый&#44; затем выкидывает/</i><br>- <i>/гзк/</i> Никогда не знаешь когда презервативы спасут твою жизнь.', 'black', 'ec0838f2', 'Pavel', '2007-06-25');

